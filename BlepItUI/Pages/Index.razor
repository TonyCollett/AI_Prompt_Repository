@page "/"
@attribute [Authorize]

<MudGrid>
    <MudItem xs="12">
        <MudPaper Class="align-center justify-center mud-width-full">
            @if(LoggedInUser != null) {
                <PromptTable LoggedInUser="LoggedInUser" />
            } 
        </MudPaper>
    </MudItem>
</MudGrid>

@code {
    [CascadingParameter(Name = "LoggedInUser")] public User LoggedInUser { get; set; }
}